#include<iostream>
using namespace std;

int main()
{
	int score;
		cout << "Please enter test score 0-100 \n";
	cin >> score;
	if (score == 100) {
			cout << "You got a perfect score \n";
		}
	if (score >= 0 && score <= 59) {
		cout << "F \n";
	}
	if (score >= 60 && score <= 69) {
		cout << "D \n";
	}
	if (score >= 70 && score <= 79) {
		cout << "C \n";
	}
	if (score >= 80 && score <= 89) {
		cout << "B \n";
	}
	if (score >= 90 && score <= 99) {
		cout << "A \n";
	}
	return 0;
}
